    "@types/uuid": "7.0.3",
    "@types/validator": "13.0.0",
    "@types/webpack-env": "1.15.2",
    "@types/yup": "0.28.0",
    "@xstate/react": "0.8.1",
    "axios": "0.19.2",
    "bcryptjs": "2.4.3",
    "cors": "2.8.5",
    "date-fns": "2.12.0",
    "dinero.js": "1.8.1",
    "dotenv": "8.2.0",
    "express-paginate": "1.0.0",
    "express-session": "1.17.1",
    "express-validator": "6.4.1",
    "faker": "4.1.0",
    "formik": "2.1.4",
    "fuse.js": "5.2.3",
    "husky": "4.2.5",
    "json": "9.0.6",
    "lowdb": "1.0.0",
    "morgan": "1.10.0",
    "nodemon": "2.0.3",
    "passport": "0.4.1",
    "passport-local": "1.0.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-infinite-calendar": "2.3.1",
    "react-number-format": "4.4.1",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "react-scripts": "3.4.1",
    "react-window": "1.8.5",
    "react-window-infinite-loader": "1.0.5",
    "shortid": "2.2.15",
    "ts-node": "8.10.1",
    "typescript": "3.8.3",
    "uuid": "8.0.0",
    "xstate": "4.9.1",
    "yup": "0.28.5"
  },
  "devDependencies": {
    "@cypress/code-coverage": "3.7.0",
    "@cypress/instrument-cra": "1.1.1",
    "chai-datetime": "1.6.0",
    "cypress": "4.5.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-cypress": "2.10.3",
    "eslint-plugin-prettier": "3.1.3",
    "prettier": "2.0.5",
    "start-server-and-test": "1.11.0"
  },
  "scripts": {
    "tsnode": "ts-node -O '{\"isolatedModules\": false, \"module\": \"commonjs\" }'",
    "tsnode:test": "NODE_ENV=test ts-node -O '{\"isolatedModules\": false, \"module\": \"commonjs\" }'",
    "types": "tsc --noEmit",
    "db:seed": "yarn tsnode scripts/generateSeedData",
    "postdb:seed": "yarn db:updatedevseed",
    "db:seed:empty": "cp data/empty-seed.json data/database.json",
    "db:seed:dev": "cp data/dev-seed.json data/database.json",
    "db:seed:test": "cp data/test-seed.json data/database.test.json",
    "db:updatedevseed": "cp data/test-seed.json data/dev-seed.json",
    "predb:updatedevseed": "yarn db:seed:test",
    "postdb:updatedevseed": "yarn db:seed:dev",
    "cypress:open": "cypress open",
    "cypress:open:mobile": "cypress open --config-file mobile-cypress.json",
    "cypress:run": "cypress run",
    "cypress:run:record": "cypress run --record",
    "prestart": "yarn db:seed:dev",
    "prestart:dev": "yarn db:seed:dev",
    "prestart:test": "yarn db:seed:test",
    "prestart:empty": "yarn db:seed:empty",
    "start": "NODE_ENV=development yarn start:api & yarn start:react",
    "dev": "yarn start:dev",
    "dev:mobile": "yarn start:dev:mobile",
    "start:dev": "NODE_ENV=development start-test start:api:watch 3001 start:react 3000 cypress:open",
    "start:dev:mobile": "NODE_ENV=development start-test start:api:watch 3001 start:react 3000 cypress:open:mobile",
    "start:empty": "NODE_ENV=development EMPTY_SEED=true start-test start:api:watch 3001 start:react 3000 cypress:open",
    "start:test": "NODE_ENV=test start-test start:api:watch 3001 start:react 3000 cypress:open",
    "start:react": "react-scripts start",
    "start:react:ci": "node scripts/testServer.js",
    "start:api": "yarn tsnode --files backend/app.ts",
    "start:api:watch": "nodemon --exec yarn tsnode --watch 'backend' backend/app.ts",
    "prebuild": "yarn types",
    "build": "react-scripts build",
    "test": "yarn cypress:open",
    "test:ci": "NODE_ENV=test yarn start:server-and-api",
    "test:headless": "yarn cypress:run",
    "test:api": "yarn cypress:run --spec 'integration/api/*'",
    "test:unit": "react-scripts test --runInBand",
    "test:unit:ci": "react-scripts test --watch false --runInBand",
    "eject": "react-scripts eject",
    "start:server-and-api": "start-test start:api 3001 start:react:ci",
    "start:server-and-api:watch": "start-test start:api:watch 3001 start:react",
    "prettier": "prettier --write '**/**.{ts,js,tsx}'",
    "list:dev:users": "cat data/database.json | json -a users | json -a id username"
  },
  "eslintConfig": {
    "env": {
      "cypress/globals": true
    },
    "extends": [
      "react-app",
      "plugin:prettier/recommended",
      "plugin:cypress/recommended"
    ],
    "plugins": [
      "cypress",
      "prettier"
    ],
    "rules": {
      "no-unused-expressions": 0
    }
  },
  "jest": {
    "watchPathIgnorePatterns": [
      "<rootDir>/data/database.json",
      "<rootDir>/data/database.test.json",
      "<rootDir>/data/test-seed.json",
      "<rootDir>/data/dev-seed.json"
    ]
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn types"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "nyc": {
    "exclude": [
      "serviceWorker.ts",
      "backend"
    ]
  }
}
