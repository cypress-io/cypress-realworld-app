  "devDependencies": {
    "cypress": "4.3.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-cypress": "2.10.3",
    "eslint-plugin-prettier": "3.1.2",
    "prettier": "2.0.4",
    "start-server-and-test": "1.10.11"
  },
  "scripts": {
    "tsnode": "ts-node -O '{\"isolatedModules\": false, \"module\": \"commonjs\" }'",
    "tsnode:test": "NODE_ENV=test ts-node -O '{\"isolatedModules\": false, \"module\": \"commonjs\" }'",
    "types": "tsc --noEmit",
    "db:seed:empty": "cp src/data/empty-seed.json src/data/database.json",
    "db:seed:dev": "cp src/data/dev-seed.json src/data/database.json",
    "db:seed:test": "cp src/data/test-seed.json src/data/database.test.json",
    "db:updatedevseed": "cp src/data/test-seed.json src/data/dev-seed.json",
    "predb:updatedevseed": "yarn db:seed:test",
    "postdb:updatedevseed": "yarn db:seed:dev",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:record": "cypress run --record",
    "prestart": "yarn db:seed:dev",
    "prestart:dev": "yarn db:seed:dev",
    "prestart:test": "yarn db:seed:test",
    "prestart:empty": "yarn db:seed:empty",
    "start": "NODE_ENV=development yarn start:api & yarn start:react",
    "dev": "yarn start:dev",
    "start:dev": "NODE_ENV=development start-test start:server-and-api:watch 3000 cypress:open",
    "start:empty": "NODE_ENV=development EMPTY_SEED=true start-test start:server-and-api:watch 3000 cypress:open",
    "start:test": "NODE_ENV=test start-test start:server-and-api:watch 3000 cypress:open",
    "start:react": "react-scripts start",
    "start:react:ci": "node scripts/testServer.js",
    "start:api": "yarn tsnode --files src/app.ts",
    "start:api:watch": "nodemon --exec yarn tsnode --watch 'src/app.ts' --watch 'src/backend' src/app.ts",
    "prebuild": "yarn types",
    "build": "react-scripts build",
    "test": "yarn cypress:open",
    "test:ci": "NODE_ENV=test yarn start:server-and-api",
    "test:headless": "yarn cypress:run",
    "test:api": "yarn cypress:run --spec 'integration/api/*'",
    "test:unit": "react-scripts test --runInBand",
    "test:unit:ci": "react-scripts test --watch false --runInBand",
    "eject": "react-scripts eject",
    "start:server-and-api": "start-test start:api 3001 start:react:ci",
    "start:server-and-api:watch": "start-test start:api:watch 3001 start:react",
    "prettier": "prettier --write '**/**.{ts,js,tsx}'"
  },
  "eslintConfig": {
    "env": {
      "cypress/globals": true
    },
    "extends": [
      "react-app",
      "plugin:prettier/recommended",
      "plugin:cypress/recommended"
    ],
    "plugins": [
      "cypress",
      "prettier"
    ]
  },
  "jest": {
    "watchPathIgnorePatterns": [
      "<rootDir>/src/data/database.json",
      "<rootDir>/src/data/database.test.json",
      "<rootDir>/src/data/test-seed.json",
      "<rootDir>/src/data/dev-seed.json"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
